		<xs:annotation>
			<xs:documentation>       
            Units of minutes
            use 0 for &amp;quot;unknown&amp;quot;
            use 65535 for &amp;quot;forever&amp;quot;
         </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedShort"/>
	</xs:simpleType>
	<!-- Descriptive Name: DE_ATIS_TimeOffset -->
	<xs:simpleType name="TimeOffset">
		<xs:annotation>
			<xs:documentation>       
            Valid time offset using signed 12 hour notation of the form HHMM
            HH=00 through 23; MM=00 through 59;
         </xs:documentation>
